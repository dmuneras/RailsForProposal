<% title "RFP's" %>

<div class= "options">
  <%= link_to "Registrar RFP", new_request_path %> |
  <%= link_to "Registrar usuario", new_user_path %>
</div>

<table>
  <tr>
    <th><%= t(:name) %></th>
    <th><%= t(:start_date) %></th>
    <th><%= t(:decision_date) %></th>
    <th><%= t(:company) %></th>
    <th><%= t(:status) %></th>
    <th><%= t(:score) %></th>
  </tr>
  <% for request in @requests %>
    <%= general_info request %>
  <% end %>
</table>


