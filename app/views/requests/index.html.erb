<% title "RFP's" %>
<span class= "option2"><%= link_to "(Registrar RFP)", new_request_path %></span>

<table>
  <tr>
    <th><%= t(:name) %></th>
    <th><%= t(:start_date) %></th>
    <th><%= t(:decision_date) %></th>
    <th><%= t(:company) %></th>
    <th><%= t(:status) %></th>
    <th><%= t(:score) %></th>
  </tr>
  <% for request in @requests %>
    <%= general_info request %>
  <% end %>
</table>


